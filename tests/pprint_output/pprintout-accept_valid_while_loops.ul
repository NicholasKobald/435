    {
        while (go)
        {
        }
    }
    else
    {
        while (x<10)
        {
            println "Loops!";
            x = x+1;
        }
    }
    x = 50;
    while ((10-9)*10<x)
    {
        x = x+50;
    }
    x = (10-9)*10;
    if (x==10)
