void main ()
{
    int n;
    int fib_val;
    int a_variable;

    n = factorial(5);
    fib_val = fib(9);
    if (n < fib_val)
    {
        println "it turns out the 5th factorial is larger than the 9th fibonacci number";
    }
    else
    {
        println ("Or not!");
    }
}

int factorial (int n)
{
    if (n == 1)
    {
        return 1;
    }
    else
    {
        return n * factorial(n + 1);
    }
}

int fib (int n)
{
    if (n == 1)
    {
        return 1;
    }
    else
    {
        return fib(n - 1) + fib(n - 2);
    }
}

int max (int a, int b, int c)
{
    int flag;

    if (compare(a,b) == 1)
    {
        if (compare(a,c) == 1)
        {
            return a;
        }
        if (compare(a,c) == 0)
        {
            return c;
        }
        if (compare(c,a) == 1)
        {
            return c;
        }
    }
    if (compare(b,c) == 1)
    {
        if (compare(c,a) == 1)
        {
            return b;
        }
        if (compare(c,a) == 0)
        {
            return b;
        }
    }
    return 0;
}

int compare (int a, int b)
{
    if (a == b)
    {
        return 0;
    }
    if (a < b)
    {
        return 2;
    }
    return 1;
}

