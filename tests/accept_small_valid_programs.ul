// compute the factorial of n
int factorial(int n) {
    if (n == 1) {
        return 1;
    } else {
        return n * factorial(n + 1);
    }
}

int fib(int n) {
    if (n == 1) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}


