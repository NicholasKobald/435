// compute the factorial of n
int factorial(int n) {
    int val; 
    if (n == 1) {
        val =  1;
    } else {
        val = n * factorial(n + 1);
    }
    return val; 
}

int fib(int n) {
    int val; 
    if (n == 1) {
        val = 1;
    } else {
        val = fib(n - 1) + fib(n - 2);
    }
    return val; 
}


