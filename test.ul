void main() {
    int x;
    int temp; 

    x = 1;

    while (x < 15) {
        println "The next number in the sequence is";
        println x;
        x = x + 1; 
        temp = x; 
        while (1 < temp) {
            temp = temp - 2;
        }
        if (1 -2 < temp) {
            if (temp == 1) {
                println "That x was even"; 
            } else {
                println "That x was odd"; 
            }
        } else {
            println "That x was even"; 
        }
    }


    println "And we done!";

}
